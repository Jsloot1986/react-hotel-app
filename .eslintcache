';\r\nimport StyledHero from '../components/StyledHero';\r\n\r\nexport default class SingleRoom extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n\r\n        this.state = {\r\n            slug: this.props.match.params.slug,\r\n            defaultBcg: defaultBcg\r\n        }\r\n    }\r\n    static contextType = RoomContext;\r\n    render() {\r\n        const { getRoom } = this.context;\r\n        const room = getRoom(this.state.slug);\r\n        if (!room) {\r\n            return (\r\n                <div className=\"error\">\r\n                <h3>Het type kamer wat u zoekt is niet beschikbaar!</h3>\r\n                <Link to=\"/rooms\" className=\"btn-primary\">\r\n                    terug naar kamers\r\n                </Link>\r\n            </div>\r\n            );\r\n        }\r\n\r\n        const {name, description, capacity, size, price, extras, breakfast, pets, images} = room\r\n        \r\n        const [mainImg, ...defaultImg] = images;\r\n        return (\r\n            <>\r\n            <StyledHero img={mainImg}>\r\n            <Banner title={`${name} room`}>\r\n                <Link to=\"/rooms\" className=\"btn-primary\">\r\n                    Terug naar kamers\r\n                </Link>\r\n            </Banner>\r\n                </StyledHero>\r\n                <section className=\"single-room\">\r\n                    <div className=\"single-room-images\">\r\n                        {defaultImg.map((item, index) => {\r\n                           return <img key={index} src={item} alt={name}/>\r\n                        })}\r\n                    </div>\r\n                    <div className=\"single-room-info\">\r\n                        <article className=\"desc\">\r\n                            <h3>details</h3>\r\n                            <p>{description}</p>\r\n                        </article>\r\n                        <article className=\"info\">\r\n                            <h3>info</h3>\r\n                            <h6> prijs : ${price}</h6>\r\n                            <h6>size: ${size} m2</h6>\r\n                            <h6>max aantal personen: {\r\n                                capacity > 1 ? `${capacity} personen` : `${capacity} persoon`}\r\n                            </h6>\r\n                            <h6>{pets ? \"huisdieren toegestaan\" : \"geen huisdieren toegestaan\"}</h6>\r\n                            <h6>{breakfast && \"met gratis ontbijt\"}</h6>\r\n                        </article>\r\n                    </div>\r\n                </section>\r\n                <section className=\"room-extras\">\r\n                    <h6>extras</h6>\r\n                    <ul className=\"extras\">\r\n                        {extras.map((item, index) => {\r\n                            return <li key={index}>~{item}</li>\r\n                        })}\r\n                    </ul>\r\n                </section>\r\n                </>\r\n        )}\r\n}\r\n","C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\pages\\Home.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\pages\\Error.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\Navbar.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\Hero.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\Banner.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\Services.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\Title.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\context.js",[],"C:\\Users\\Gebruiker\\.vscode\\extensions\\projects\\Winc_Academy\\React\\hotel-app\\src\\components\\FeaturedR